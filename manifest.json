{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "extensions.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://habr.com/*" ],
      "js": [ "app.js" ]
    }
  ]
}